{
    "version": 2,
    "functions": {
        "api/index.py": {
            "includeFiles": "storage/events.json"
        }
    },
    "rewrites": [
        {
            "source": "/api",
            "destination": "/api/index.py"
        },
        {
            "source": "/api/(.*)",
            "destination": "/api/index.py"
        }
    ]
}