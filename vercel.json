    "functions": {
        "api/main.py": {
            "includeFiles": "storage/events.json"
        }
    },
    "rewrites": [
        {
            "source": "/api",
            "destination": "api/main.py"
        },
        {
            "source": "/api/(.*)",
            "destination": "api/main.py"
        }
    ]
}